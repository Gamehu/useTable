{
  "compilerOptions": {
    "target": "ES2019",
    "module": "esnext",
    "declaration": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  }
}
